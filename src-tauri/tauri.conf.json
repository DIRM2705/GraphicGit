{
  "build": {
    "devPath": "../Frontend",
    "distDir": "../Frontend",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "BaueLogika",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "open": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "copyright": "2024",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.prehilabs.bauelogika",
      "publisher": "PrehiLabs",
      "targets": "msi"
      },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "title": "Home",
        "minimizable": true,
        "maximizable": false,
        "resizable": false,
        "center": true,
        "height": 600,
        "width": 500, 
        "url": "index.html", 
        "visible": true,  
        "fullscreen": false
      },
      {
        "label": "git-url",
        "title": "URL del repositorio",
        "minimizable": false,
        "maximizable": false,
        "closable": true,
        "resizable": false,
        "center": true,
        "height": 150,
        "width": 600, 
        "url": "git-url.html", 
        "visible": false,
        "alwaysOnTop": true, 
        "fullscreen": false
      }
    ]
  }
}
