{
  "build": {
    "devPath": "../Frontend",
    "distDir": "../Frontend",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Grafic Git",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "open": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "copyright": "2024",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.prehilabs.bauelogika",
      "publisher": "PrehiLabs",
      "targets": "msi"
      },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "title": "Graphic Git",
        "minimizable": true,
        "maximizable": false,
        "resizable": false,
        "center": true,
        "height": 650,
        "width": 550, 
        "url": "index.html", 
        "visible": true,  
        "fullscreen": false
      }
    ]
  }
}
